{
  "name": "manage-your-appointments",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^22.21.1",
    "npm": ">=10.7.0"
  },
  "scripts": {
    "prepare": "cd ../../ && husky src/client/.husky",
    "tsc": "tsc --noEmit",
    "dev": "next dev",
    "build": "next build",
    "start": "next build && next start",
    "clean:next": "rimraf .next",
    "clean:full": "rimraf node_modules .next",
    "lint": "eslint **/*.{ts,tsx} --max-warnings=0",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "test": "jest",
    "test:utc:tz": "TZ=UTC jest",
    "test:ci": "jest --reporters=jest-junit --reporters=default",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@microsoft/applicationinsights-web": "^3.3.10",
    "@next/env": "^15.5.9",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.208.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.208.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.208.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@opentelemetry/sdk-logs": "^0.208.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@vercel/otel": "^2.1.0",
    "dayjs": "^1.11.19",
    "file-saver": "^2.0.5",
    "manage-your-appointments": "file:",
    "next": "^15.5.9",
    "nhsuk-frontend": "^9.6.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.68.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@azure/cosmos": "^4.9.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "dotenv": "^16.6.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.2.0",
    "jest-fixed-jsdom": "^0.0.11",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8",
    "prettier": "^3.7.4",
    "rimraf": "^6.0.1",
    "sass": "^1.96.0",
    "stylelint": "^15.10.2",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-prettier": "^4.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write --list-different"
    ],
    "*.{scss,css}": [
      "stylelint --fix",
      "prettier --write --list-different"
    ]
  },
  "overrides": {
    "glob": "10.4.5"
  }
}
